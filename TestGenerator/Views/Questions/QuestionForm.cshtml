@model TestGenerator.Core.ViewModels.Test.QuestionFormViewModel

        <div class="form-group">
            @Html.LabelFor(x => Model.Text)
            @Html.TextBoxFor(x => Model.Text, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(x => Model.Type)
            @Html.DropDownListFor(x => Model.Type, new SelectList(Model.QuestionTypes, "Id", "Name"), "", new { @class = "form-control target" })
        </div>
<button type="button" class="clas32" id="clas32">Добавить ответ</button>
